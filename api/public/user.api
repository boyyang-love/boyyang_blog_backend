syntax = "v1"


type (
    User {
        Id uint `json:"id"`
        Username string `json:"username"`
        Gender int `json:"gender"`
        AvatarUrl string `json:"avatar_url"`
        Tel int `json:"tel"`
        BackgroundImage string `form:"background_image" json:"background_image"`
        Motto *string `form:"motto" json:"motto"`
    }
)